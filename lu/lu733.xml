    <subCorpus>
        <sentence ID="3835">
            <text>Mais les dockers n' entendent pas consentir d' autres " sacrifices " .</text>
            <annotationSet status="MANUAL" ID="5291">
                <layer rank="1" name="FE">
                    <label end="67" start="44" name="Proposed_action"/>
                    <label end="15" start="5" name="Cognizer"/>
