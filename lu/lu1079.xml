                <layer rank="1" name="Target">
                    <label end="16" start="5" name="Target"/>
                </layer>
            </annotationSet>
        </sentence>
        <sentence ID="16344">
            <text>Pourrait -il y avoir une manifestation plus convaincante du fait que Dieu aime l' homme " .</text>
            <annotationSet status="MANUAL" ID="20520">
                <layer rank="1" name="FE">
