                <layer rank="1" name="Target">
                    <label end="199" start="193" name="Target"/>
                </layer>
            </annotationSet>
        </sentence>
        <sentence ID="11097">
            <text>Le gouvernement avait alors presque bloqué les prix pour contrer les effets inflationnistes du retour d' un million de rapatriés d' Algérie .</text>
            <annotationSet status="MANUAL" ID="15356">
