                <layer rank="1" name="Target">
                    <label end="130" start="118" name="Target"/>
                </layer>
            </annotationSet>
        </sentence>
        <sentence ID="13093">
            <text>La BBC s' engage à " rendre des comptes " à son public .</text>
            <annotationSet status="MANUAL" ID="17465">
                <layer rank="1" name="FE">
                </layer>
                <layer rank="1" name="Target">
