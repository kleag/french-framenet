                <layer rank="1" name="Target">
                    <label end="124" start="117" name="Target"/>
                </layer>
            </annotationSet>
        </sentence>
        <sentence ID="1871">
            <text>Bien sûr , les 800 000 tonnes d' aide d' urgence promises par la communauté internationale doivent être envoyées au plus vite .</text>
            <annotationSet status="MANUAL" ID="2273">
                <layer rank="1" name="FE">
