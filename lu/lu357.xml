                <layer rank="1" name="FE">
                    <label end="18" start="0" name="Speaker"/>
                    <label end="186" start="39" name="Eventuality"/>
                </layer>
                <layer rank="1" name="Target">
                    <label end="33" start="28" name="Target"/>
                </layer>
            </annotationSet>
        </sentence>
        <sentence ID="10819">
            <text>En contrepartie , la Banque centrale promet de baisser immédiatement ses taux .</text>
