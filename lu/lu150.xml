                    <label end="158" start="142" name="Category"/>
                    <label itype="ENI" name="Cognizer"/>
                    <label itype="ENI" name="Item"/>
                </layer>
                <layer rank="1" name="Target">
                    <label end="158" start="145" name="Target"/>
                </layer>
            </annotationSet>
        </sentence>
