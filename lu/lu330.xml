                <layer rank="1" name="Target">
                    <label end="150" start="144" name="Target"/>
                </layer>
            </annotationSet>
        </sentence>
        <sentence ID="8182">
            <text>Toutefois , le prix de la cession n' a pas été communiqué .</text>
