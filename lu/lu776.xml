                <layer rank="1" name="Target">
                    <label end="69" start="55" name="Target"/>
                </layer>
            </annotationSet>
        </sentence>
        <sentence ID="15714">
            <text>" La recommandation du client satisfait " devient un leitmotiv apparemment suffisant pour attirer les candidats .</text>
            <annotationSet status="MANUAL" ID="19902">
                <layer rank="1" name="FE">
                    <label end="40" start="23" name="Persuader"/>
