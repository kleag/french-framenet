                <layer rank="1" name="Target">
                    <label end="19" start="6" name="Target"/>
                </layer>
            </annotationSet>
        </sentence>
        <sentence ID="6041">
            <text>Le CIFUS transmettra sa recommandation à la Maison Blanche le 7 juillet .</text>
            <annotationSet status="MANUAL" ID="8959">
                <layer rank="1" name="FE">
                    <label end="57" start="41" name="Cognizer"/>
                    <label end="22" start="21" name="Persuader"/>
