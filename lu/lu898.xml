    <subCorpus>
        <sentence ID="2404">
            <text>Le déclenchement de l' affaire</text>
            <annotationSet status="MANUAL" ID="3139">
                <layer rank="1" name="FE">
                    <label end="29" start="17" name="Effect"/>
                    <label itype="DNI" name="Cause"/>
                </layer>
