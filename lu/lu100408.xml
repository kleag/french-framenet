    <subCorpus>
        <sentence ID="1584">
            <text>Nous devons considérer les personnes qui votent pour ces partis et comprendre pourquoi cette situation voit le jour .</text>
            <annotationSet status="MANUAL" ID="1657">
                <layer rank="1" name="FE">
                </layer>
