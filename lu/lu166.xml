                    <label itype="DNI" name="Cause"/>
                </layer>
                <layer rank="1" name="Target">
                    <label end="14" start="0" name="Target"/>
                </layer>
            </annotationSet>
        </sentence>
        <sentence ID="1777">
            <text>C' est pourquoi notre résolution demande la levée de cet embargo .</text>
            <annotationSet status="MANUAL" ID="2037">
                <layer rank="1" name="FE">
                    <label end="65" start="16" name="Effect"/>
                    <label itype="DNI" name="Cause"/>
                </layer>
                <layer rank="1" name="Target">
