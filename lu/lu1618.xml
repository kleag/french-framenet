                    <label end="100" start="77" name="Speaker"/>
                    <label itype="UNI" name="Topic"/>
                </layer>
                <layer rank="1" name="Target">
                    <label end="140" start="132" name="Target"/>
                </layer>
            </annotationSet>
        </sentence>
        <sentence ID="2184">
            <text>Une jeune femme témoigne , les larmes au yeux , devant le congrès américain .</text>
            <annotationSet status="MANUAL" ID="2875">
                <layer rank="1" name="FE">
