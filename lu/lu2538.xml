    <subCorpus>
        <sentence ID="388">
            <text>Mais peu avant 14 h , la jeune fille prévenait sa famille qu' elle était saine et sauve et qu' elle se trouvait à Lyon .</text>
            <annotationSet status="MANUAL" ID="501">
                <layer rank="1" name="FE">
                    <label end="89" start="62" name="Message"/>
                    <label end="117" start="95" name="Message"/>
                    <label end="56" start="47" name="Addressee"/>
                    <label end="35" start="22" name="Speaker"/>
                </layer>
                <layer rank="1" name="Target">
                    <label end="45" start="37" name="Target"/>
                </layer>
            </annotationSet>
        </sentence>
        <sentence ID="1015">
            <text>Vous devez prévenir votre médecin :</text>
            <annotationSet status="MANUAL" ID="1067">
                <layer rank="1" name="FE">
                    <label end="32" start="20" name="Addressee"/>
                    <label end="3" start="0" name="Speaker"/>
                </layer>
                <layer rank="1" name="Target">
                    <label end="18" start="11" name="Target"/>
                </layer>
            </annotationSet>
        </sentence>
        <sentence ID="1057">
            <text>Lorsque vous êtes à l' hôpital : prévenez immédiatement votre médecin ou votre infirmière .</text>
            <annotationSet status="MANUAL" ID="1106">
                <layer rank="1" name="FE">
                    <label end="29" start="8" name="Message"/>
                    <label end="88" start="56" name="Addressee"/>
                    <label end="11" start="8" name="Speaker"/>
                </layer>
                <layer rank="1" name="Target">
                    <label end="40" start="33" name="Target"/>
                </layer>
            </annotationSet>
        </sentence>
        <sentence ID="1489">
            <text>Prévenez votre médecin ou votre dentiste si vous présentez de tels symptômes .</text>
            <annotationSet status="MANUAL" ID="1497">
                <layer rank="1" name="FE">
                    <label end="75" start="44" name="Message"/>
