    <subCorpus>
        <sentence ID="1551">
            <text>Très brièvement , il s' agit de limiter les émissions de CO2 imputables à l' homme .</text>
            <annotationSet status="MANUAL" ID="1579">
                <layer rank="1" name="FE">
                    <label end="59" start="40" name="Known_state_of_affairs"/>
