    <subCorpus>
        <sentence ID="333">
            <text>Une sorte de réaction épidermique par rapport à leur présence " commentait le commissaire central .</text>
            <annotationSet status="MANUAL" ID="427">
                <layer rank="1" name="FE">
                    <label end="60" start="0" name="Message"/>
