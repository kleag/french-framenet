    <subCorpus>
        <sentence ID="2593">
            <text>Il aurait expliqué aux enquêteurs qu' il s' agissait d' un prêt personnel affirmant même l' avoir remboursé .</text>
            <annotationSet status="MANUAL" ID="3451">
                <layer rank="1" name="FE">
                    <label end="1" start="0" name="Loaner"/>
                    <label end="90" start="89" name="Loan"/>
