                    <label end="13" start="0" name="Effect"/>
                </layer>
                <layer rank="1" name="Target">
                    <label end="42" start="34" name="Target"/>
                </layer>
            </annotationSet>
        </sentence>
        <sentence ID="9749">
            <text>Ses compétences sont plus larges que dans les autres régions , par suite du nouveau statut de la Corse .</text>
            <annotationSet status="MANUAL" ID="13712">
                <layer rank="1" name="FE">
                    <label end="101" start="76" name="Cause"/>
