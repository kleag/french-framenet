                    <label itype="UNI" name="Speaker"/>
                </layer>
                <layer rank="1" name="Target">
                    <label end="67" start="57" name="Target"/>
                </layer>
            </annotationSet>
        </sentence>
        <sentence ID="1877">
            <text>Bien sûr , pour l' heure , c' est simplement l' appel de la population qui souffre que nous devons entendre .</text>
            <annotationSet status="MANUAL" ID="2289">
                <layer rank="1" name="FE">
